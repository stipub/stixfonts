    \begingroup \catcode`\"=12
    \gdef\newmcodes@{%
      \mathcode`\'="007F\relax%
      \mathcode`\*="003C\relax%
      \mathcode`\.="613A\relax%
      \ifnum\mathcode`\-="002A \else
        \mathchardef\std@minus\mathcode`\-\relax
